<% form_for resource_name, resource, :url => registration_path(resource_name), :html => { } do |f| -%>
  <fieldset class="ui-widget-content">
  <legend class="ui-widget-header ui-corner-all mLeft10 pAll5" >Sign up</legend>
  <div class="mLeft10">
    <%= f.error_messages %>
    <p><%= f.label :email %></p>
    <p><%= f.text_field :email, :class => "iText ui-widget-content ui-corner-all", :size => 60 %></p>

    <p><%= f.label :password %></p>
    <p><%= f.password_field :password, :class => "iText ui-widget-content ui-corner-all", :size => 60 %></p>

    <p><%= f.label :password_confirmation %></p>
    <p><%= f.password_field :password_confirmation, :class => "iText ui-widget-content ui-corner-all", :size => 60 %></p>

    <p><%= f.submit "Sign up", :class => 'ui-state-hover ui-corner-all pAll5' %></p>
  </div>
  </fieldset>
<% end -%>

<%= render :partial => "devise/shared/links" %>
